	<!-- Boomalope -->
	<ThingDef ParentName="AnimalThingBase">
		<defName>GR_Boomnotherium</defName>
		<label>boomnotherium</label>
		<description>The Boomnotherium is an hybrid between a boomalope and a large prehistoric relative of modern-day elephants. In latin, boomnotherium roughly translates as "terrible beast that will explode in your ass".\n\nUseful abilities: Boomnotheriums produce huge amounts of chemfuel, and they are "tanky", able to withstand a lot of abuse before exploding.</description>
		<statBases>
			<MoveSpeed>4.3</MoveSpeed>
			<ComfyTemperatureMin>-25</ComfyTemperatureMin>
			<MarketValue>1600</MarketValue>
			<FilthRate>16</FilthRate>
		</statBases>
		<tools>
			<li>
				<label>tusk</label>
				<cooldownTime>2.3</cooldownTime>
				<power>32</power>
				<capacities>
					<li>Scratch</li>
					<li>Stab</li>
				</capacities>
				<linkedBodyPartsGroup>TuskAttackTool</linkedBodyPartsGroup>
			</li>
			<li>
				<label>left foot</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>16</power>
				<cooldownTime>1.75</cooldownTime>
				<linkedBodyPartsGroup>FrontLeftLeg</linkedBodyPartsGroup>
			</li>
			<li>
				<label>right foot</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>16</power>
				<cooldownTime>1.75</cooldownTime>
				<linkedBodyPartsGroup>FrontRightLeg</linkedBodyPartsGroup>
			</li>
			<li>
				<label>head</label>
				<cooldownTime>1.65</cooldownTime>
				<power>13</power>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<chanceFactor>0.01</chanceFactor>
				<linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
			</li>
		</tools>
		<comps>
			<li Class="AnimalBehaviours.CompProperties_AnimalProduct">
				<resourceDef>GR_CompressedChemfuel</resourceDef>
				<gatheringIntervalDays>2</gatheringIntervalDays>
				<resourceAmount>20</resourceAmount>
				<customResourceString>Cryofuel</customResourceString>
			</li>
			
		</comps>
		<race>
		
			<herdMigrationAllowed>false</herdMigrationAllowed>
			<deathActionWorkerClass>GeneticRim.DeathActionWorker_BiggerExplosion</deathActionWorkerClass>
			<executionRange>6</executionRange>
			<herdAnimal>true</herdAnimal>
			<predator>false</predator>
			<body>QuadrupedAnimalWithHoovesTusksAndTrunk</body>
			<baseHungerRate>1.8</baseHungerRate>
			<baseBodySize>5.5</baseBodySize>
			<baseHealthScale>8.5</baseHealthScale>
			<foodType>VegetarianRoughAnimal</foodType>
			<gestationPeriodDays>10</gestationPeriodDays>
			<packAnimal>false</packAnimal>
			<wildness>0.75</wildness>
			<lifeExpectancy>4</lifeExpectancy>
			<trainability>None</trainability>
			<roamMtbDays>2</roamMtbDays>
			<manhunterOnTameFailChance>0.05</manhunterOnTameFailChance>
			<manhunterOnDamageChance>0.15</manhunterOnDamageChance>
			<useLeatherFrom>Elephant</useLeatherFrom>
			<useMeatFrom>Elephant</useMeatFrom>
			<lifeStageAges>
				<li>
					<def>AnimalBaby</def>
					<minAge>0</minAge>
				</li>
				<li>
					<def>AnimalJuvenile</def>
					<minAge>0.4</minAge>
				</li>
				<li>
					<def>AnimalAdult</def>
					<minAge>0.8</minAge>
					<soundCall>Pawn_Elephant_Call</soundCall>
					<soundAngry>Pawn_Elephant_Angry</soundAngry>
					<soundWounded>Pawn_Elephant_Injured</soundWounded>
					<soundDeath>Pawn_Elephant_Dead</soundDeath>
				</li>
			</lifeStageAges>
			<soundMeleeHitPawn>Pawn_Melee_BigBash_HitPawn</soundMeleeHitPawn>
			<soundMeleeHitBuilding>Pawn_Melee_BigBash_HitBuilding</soundMeleeHitBuilding>
			<soundMeleeMiss>Pawn_Melee_BigBash_Miss</soundMeleeMiss>
		</race>
		<tradeTags>
			<li>AnimalGenetic</li>
		</tradeTags>
		<modExtensions>
			<li Class="AnimalBehaviours.AnimalStatExtension">
				<statToAdd>
					<li>GR_HybridRole</li>
				</statToAdd>
				<statValues>
					<li>GR_HybridRoleFarm</li>
				</statValues>
				<statDescriptions>
					<li>GR_HybridRoleFarmDesc</li>
				</statDescriptions>
			</li>
		</modExtensions>
