<Defs>
	<!-- Bear -->
	<ThingDef ParentName="AnimalThingBase">
		<defName>GR_Elasmobearium</defName>
		<label>elasmobearium</label>
		<description>The Elasmobearium is a hybrid of a bear and the ancient ancestor of the rhinoceros. The Elasmobearium is a pretty aggressive omnivore, using its horn to impale its victims and then mauling them with their vicious paws.\n\n&lt;color=#E5E54C&gt;Gameplay effect:&lt;/color&gt; All primary bear hybrids have stun attacks. Aside from obviously being a very powerful creature, they produce a wool of excellent quality.</description>
		<statBases>
			<MoveSpeed>4.5</MoveSpeed>
			<ComfyTemperatureMin>-85</ComfyTemperatureMin>
			<MarketValue>1300</MarketValue>
			<FilthRate>4</FilthRate>
		</statBases>
		<tools>
			<li>
				<label>left claw</label>
				<capacities>
					<li>Scratch</li>
				</capacities>
				<power>17</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>FrontLeftPaw</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>14</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>right claw</label>
				<capacities>
					<li>Scratch</li>
				</capacities>
				<power>17</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>FrontRightPaw</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>14</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<capacities>
					<li>Bite</li>
				</capacities>
				<power>23.6</power>
				<cooldownTime>2.6</cooldownTime>
				<linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
				<chanceFactor>0.5</chanceFactor>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>14</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>horn</label>
				<capacities>
					<li>Scratch</li>
					<li>Stab</li>
				</capacities>
				<power>20</power>
				<cooldownTime>1.7</cooldownTime>
				<linkedBodyPartsGroup>HornAttackTool</linkedBodyPartsGroup>
			</li>
		</tools>
		<comps>
			<li Class="CompProperties_Shearable">
				<woolDef>GR_ThickHybridWool</woolDef>
				<shearIntervalDays>35</shearIntervalDays>
				<woolAmount>210</woolAmount>
			</li>
		</comps>
		<race>
			<herdMigrationAllowed>false</herdMigrationAllowed>
			<herdAnimal>true</herdAnimal>
			<predator>true</predator>
			<body>GR_QuadrupedAnimalWithPawsAndHorn</body>
			<baseHungerRate>0.55</baseHungerRate>
			<baseBodySize>4</baseBodySize>
			<baseHealthScale>6.5</baseHealthScale>
			<foodType>OmnivoreAnimal, OvivoreAnimal</foodType>
			<packAnimal>false</packAnimal>
			<wildness>0.80</wildness>
			<lifeExpectancy>4</lifeExpectancy>
			<trainability>Advanced</trainability>
			<gestationPeriodDays>20</gestationPeriodDays>
			<manhunterOnTameFailChance>0.2</manhunterOnTameFailChance>
			<manhunterOnDamageChance>1</manhunterOnDamageChance>
			<useLeatherFrom>Rhinoceros</useLeatherFrom>
			<useMeatFrom>Rhinoceros</useMeatFrom>
			<nuzzleMtbHours>45</nuzzleMtbHours>
			<lifeStageAges>
				<li>
					<def>AnimalBaby</def>
					<minAge>0</minAge>
				</li>
				<li>
					<def>AnimalJuvenile</def>
					<minAge>0.4</minAge>
				</li>
				<li>
					<def>AnimalAdult</def>
					<minAge>0.8</minAge>
					<soundCall>Pawn_Rhinoceros_Call</soundCall>
					<soundAngry>Pawn_Rhinoceros_Angry</soundAngry>
					<soundWounded>Pawn_Rhinoceros_Wounded</soundWounded>
					<soundDeath>Pawn_Rhinoceros_Death</soundDeath>
				</li>
			</lifeStageAges>
			<soundMeleeHitPawn>Pawn_Melee_BigBash_HitPawn</soundMeleeHitPawn>
			<soundMeleeHitBuilding>Pawn_Melee_BigBash_HitBuilding</soundMeleeHitBuilding>
			<soundMeleeMiss>Pawn_Melee_BigBash_Miss</soundMeleeMiss>
		</race>
		<tradeTags>
			<li>AnimalGenetic</li>
		</tradeTags>
		<modExtensions>
			<li Class="AnimalBehaviours.AnimalStatExtension">
				<statToAdd>
					<li>GR_HybridRole</li>
				</statToAdd>
				<statValues>
					<li>GR_HybridRoleCombat</li>
				</statValues>
				<statDescriptions>
					<li>GR_HybridRoleCombatDesc</li>
				</statDescriptions>
			</li>
		</modExtensions>
	</ThingDef>
