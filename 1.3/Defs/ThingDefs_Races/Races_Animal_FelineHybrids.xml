				<statValues>
					<li>GR_HybridRoleCombat</li>
				</statValues>
				<statDescriptions>
					<li>GR_HybridRoleCombatDesc</li>
				</statDescriptions>
			</li>
		</modExtensions>
	</ThingDef>
